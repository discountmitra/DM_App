{
  "expo": {
    "name": "Discount Mitra",
    "slug": "discount-mitra",
    "version": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "9d7d15a8-b44d-48ca-8d70-fd83b25cc44b"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "discountmitra",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.discountmitra.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.discountmitra.app",
      "versionCode": 1,
      "usesCleartextTraffic": false,
      "networkSecurityConfig": "network_security_config.xml"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
